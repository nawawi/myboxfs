make distclean
./configure --prefix=/usr --disable-nsl --build=i686-mybox-linux --host=i686-mybox-linux \
--exec-prefix=/usr \
--bindir=/bin \
--sbindir=/bin \
--libexecdir=/bin \
--sysconfdir=/etc \
--datadir=/usr/share \
--localstatedir=/var \
--mandir=/usr/man \
--infodir=/usr/info \
--disable-lastlog --disable-utmp \
--disable-utmpx --disable-wtmp --disable-wtmpx \
--without-x
