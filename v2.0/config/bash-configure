make distclean
./configure --prefix=/usr --disable-nsl --build=i686-mybox-linux --host=i686-mybox-linux \
--bindir=/bin \
--sbindir=/bin \
--sysconfdir=/etc \
--disable-help-builtin \
--disable-restricted \
--enable-static-link \
--without-bash-malloc \
--disable-postgresql --disable-mysql \
--disable-gdbm --disable-expat --disable-libwebserver \
--disable-gtk2
