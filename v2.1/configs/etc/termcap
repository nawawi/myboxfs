	:UP=\E&a-%dR:ch=\E&a%dC:cl=\E&a0c0Y\EJ:cm=\E&a%dy%dC:\
	:cv=\E&a%dY:ho=\E&a0y0C:ll=\E&a0y0C\EA:tc=newhp:
